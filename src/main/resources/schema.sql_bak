CREATE TABLE USERS(
    ID BIGINT GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    USERNAME VARCHAR(255),
    PASSWORD VARCHAR(255),
    FIRSTNAME VARCHAR(255),
    LASTNAME VARCHAR(255),
    EMAIL VARCHAR(255)
);

CREATE TABLE IMAGE(
    IMAGEID BIGINT GENERATED BY DEFAULT AS IDENTITY PRIMARY KEY,
    IMGUR_ID VARCHAR(255),
    DELETE_HASH VARCHAR(255),
    TITLE VARCHAR(255),
    URL VARCHAR(255),
    USER_ID BIGINT,
    CONSTRAINT FK_IMAGE_USER FOREIGN KEY (USER_ID) REFERENCES USER(ID)
);